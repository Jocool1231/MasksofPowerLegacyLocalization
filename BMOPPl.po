msgctxt "Dialogue_Response,DL_StaffSeller1_PlayerResponse_2"
msgid "I'm allowed to buy one?"
msgstr "Mogę kupić jedną?"

#. Key:	DL_StaffSeller1_PlayerResponse_3
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/Ta-Koro/Quartermaster/DL_StaffSeller1.Default__DL_StaffSeller1_C.Responses(2).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
#: /Game/BluePrints/Classes/Dialogue/Lines/Ta-Koro/Quartermaster/DL_StaffSeller1.Default__DL_StaffSeller1_C.Responses(2).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
msgctxt "Dialogue_Response,DL_StaffSeller1_PlayerResponse_3"
msgid "Not at the moment no."
msgstr "Nie, na razie nie."

#. Key:	DL_StaffSeller2_PlayerResponse_1
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/Ta-Koro/Quartermaster/DL_StaffSeller2.Default__DL_StaffSeller2_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
