msgctxt "Dialogue_Response,DL_JobmasterReturn3_PlayerResponse_1"
msgid "I believe you owe me some widgets."
msgstr "Chyba należy mi się trochę widżetów."

#. Key:	DL_JobmasterReturn_PlayerResponse_1
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/PoKoro/MapQuest/DL_JobmasterReturn.Default__DL_JobmasterReturn_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
