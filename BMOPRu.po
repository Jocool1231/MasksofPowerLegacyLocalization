msgctxt "Dialogue_Response,DL_VakamaBeforeRescueLeave_PlayerResponse_1"
msgid "I shall leave you in peace."
msgstr "Я оставлю вас в покое."

#. Key:	DL_VakamaBeforeRescueMap_PlayerResponse_1
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/Ta-Koro/Turaga/DL_VakamaBeforeRescueMap.Default__DL_VakamaBeforeRescueMap_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
#: /Game/BluePrints/Classes/Dialogue/Lines/Ta-Koro/Turaga/DL_VakamaBeforeRescueMap.Default__DL_VakamaBeforeRescueMap_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
msgctxt "Dialogue_Response,DL_VakamaBeforeRescueMap_PlayerResponse_1"
msgid "I'll find the other Turaga."
msgstr "Я найду остальных Турага."

#. Key:	DL_VakamaBeforeRescueWhatDo_PlayerResponse_1
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/Ta-Koro/Turaga/DL_VakamaBeforeRescueWhatDo.Default__DL_VakamaBeforeRescueWhatDo_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
