msgctxt "Dialogue_Response,DL_PoduRaceReturnKopakaPohatu2_PlayerResponse_1"
msgid "I’ll indulge in your little game now."
msgstr "Asseconderò il tuo giochetto."

#. Key:	DL_PoduRaceReturnPohatu2_PlayerResponse_1
#. SourceLocation:	/Game/BluePrints/Classes/Dialogue/Lines/RaceQuest/DeclineReturn/DL_PoduRaceReturnPohatu2.Default__DL_PoduRaceReturnPohatu2_C.Responses(0).Responses.ResponseText_20_B7867F0F4B34A10C5D626B84DD15AB04
